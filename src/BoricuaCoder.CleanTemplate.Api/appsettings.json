{
  "ConnectionStrings": {
    "DefaultConnection": "Host=localhost;Port=5432;Database=cleantemplate;Username=postgres;Password=postgres"
  },
  "Omdb": {
    "ApiKey": "YOUR_OMDB_API_KEY"
  },
  "CoreSetup": {
    "Jwt": {
      "Authority": "https://your-identity-provider.com/realms/your-realm",
      "Audience": "your-api-audience",
      "RequireHttpsMetadata": true
    },
    "Swagger": {
      "AuthorizationUrl": "https://your-identity-provider.com/realms/your-realm/protocol/openid-connect/auth",
      "TokenUrl": "https://your-identity-provider.com/realms/your-realm/protocol/openid-connect/token",
      "Scopes": {
        "openid": "OpenID",
        "profile": "Profile"
      }
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  }
}
